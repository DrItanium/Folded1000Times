actor StrikerClip : Ammo 
{
    Inventory.PickupMessage "Don't think you should've gotten this."
    Inventory.Amount 1
    Inventory.MaxAmount 7
    Ammo.BackpackAmount 0
    Ammo.BackpackMaxAmount 7
    +IGNORESKILL
    Inventory.Icon "ABULC0"
    States
    {
    Spawn:
        NOPE A 1
        stop
    }
}

