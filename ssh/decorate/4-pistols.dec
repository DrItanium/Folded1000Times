        TNT1 AA 0 A_FireCustomMissile("PistolBulletTracer",random(-325,325)/100.00,1,0,1,0,random(-200,200)/100.00)
    AltHold2Finish:
        TNT1 A 0 A_JumpIfInventory("GrabbingTheWall",1,2)
        TNT1 A 0 A_Recoil(0.1)
        TNT1 A 0
        TNT1 A 0 A_JumpIfInventory("IAmASillyPersonWhoDoesntLikeRecoil",1,"AltHold2NoRecoil")
        TNT1 A 0 A_SetAngle(angle+(random(-300,300)/1000.000))
        TNT1 A 0 A_SetPitch(pitch-(random(200,450)/1000.000))
    AltHold2NoRecoil:
        W2PD A 1 A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)
        TNT1 A 0 A_JumpIfInventory("SpecialistMagazine",2,"AltHold3")
        goto AltHoldOneGunFinish
    AltHold3:
        TNT1 A 0 A_JumpIfInventory("RangedModeOn",1,"AltHold3Tracer")
        W2PD R 1 BRIGHT A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)
        TNT1 A 0 A_Light1
        TNT1 A 0 A_AlertMonsters
        TNT1 A 0 A_PlayWeaponSound("special/fire")
        TNT1 A 0 A_FireBullets(0,0,1,0,"BlankPuff", FBF_NORANDOM )
        TNT1 AA 0 A_FireCustomMissile("PistolBullet",random(-325,325)/100.00,1,0,1,0,random(-200,200)/100.00)
        goto AltHold3Finish
    AltHold3Tracer:
        W2PD W 1 BRIGHT A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)
        TNT1 A 0 A_Light1
        TNT1 A 0 A_AlertMonsters
        TNT1 A 0 A_PlayWeaponSound("special/fire")
        TNT1 A 0 A_FireBullets(0,0,1,0,"BlankPuff", FBF_NORANDOM )
        TNT1 AA 0 A_FireCustomMissile("PistolBulletTracer",random(-325,325)/100.00,1,0,1,0,random(-200,200)/100.00)
    AltHold3Finish:
        TNT1 A 0 A_JumpIfInventory("GrabbingTheWall",1,2)
        TNT1 A 0 A_Recoil(0.1)
        TNT1 A 0
        TNT1 A 0 A_JumpIfInventory("IAmASillyPersonWhoDoesntLikeRecoil",1,"AltHold3Finish2")
        TNT1 A 0 A_SetAngle(angle+(random(-300,300)/1000.000))
        TNT1 A 0 A_SetPitch(pitch-(random(200,450)/1000.000))
    AltHold3Finish2:
        W2PD S 1 A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)
        TNT1 A 0 A_Light0
        TNT1 A 0 A_ClearRefire
        W2PD TUUVVA 1 A_WeaponReady(WRF_NOSWITCH)
        goto AltFireFinish

    AltHoldOneGunFinish:
        TNT1 A 0 A_JumpIfNoAmmo("AltHoldNope")
        TNT1 A 0 A_JumpIfInventory("RangedModeOn",1,"AltHoldOneGunFinishTracer")
        W2PD G 1 BRIGHT A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)
        TNT1 A 0 A_TakeInventory("PistolFired",1)
        TNT1 A 0 A_Light1
        TNT1 A 0 A_AlertMonsters
        TNT1 A 0 A_PlayWeaponSound("special/fireleft")
        TNT1 A 0 A_FireBullets(0,0,1,0,"BlankPuff", FBF_NORANDOM )
        TNT1 A 0 A_FireCustomMissile("PistolBullet",random(-325,325)/100.00,1,0,1,0,random(-200,200)/100.00)
        goto AltHoldOneGunFinish2
    AltHoldOneGunFinishTracer:
        W2PD Q 1 BRIGHT A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)
        TNT1 A 0 A_TakeInventory("PistolFired",1)
        TNT1 A 0 A_Light1
        TNT1 A 0 A_AlertMonsters
        TNT1 A 0 A_PlayWeaponSound("special/fireleft")
        TNT1 A 0 A_FireBullets(0,0,1,0,"BlankPuff", FBF_NORANDOM )
        TNT1 A 0 A_FireCustomMissile("PistolBulletTracer",random(-325,325)/100.00,1,0,1,0,random(-200,200)/100.00)
    AltHoldOneGunFinish2:
        TNT1 A 0 A_JumpIfInventory("GrabbingTheWall",1,2)
        TNT1 A 0 A_Recoil(0.1)
        TNT1 A 0
        TNT1 A 0 A_JumpIfInventory("IAmASillyPersonWhoDoesntLikeRecoil",1,"AltHoldOneGunFinishNoRecoil")
        TNT1 A 0 A_SetAngle(angle+(random(-300,300)/1000.000))
        TNT1 A 0 A_SetPitch(pitch-(random(200,450)/1000.000))
    AltHoldOneGunFinishNoRecoil:
        W2PD H 1 A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)
        TNT1 A 0 A_Light0
        TNT1 A 0 A_ClearRefire
        W2PD IJJKKA 1 A_WeaponReady(WRF_NOSWITCH)
        goto ReloadCheck
    AltHoldNope:
        TNT1 A 0 A_PlaySoundEx("special/dryfire","soundslot7",0,0)
        W2PD S 1 A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)
        TNT1 A 0 A_Light0
        TNT1 A 0 A_ClearRefire
        W2PD TUUVVA 1 A_WeaponReady(WRF_NOSWITCH)
        goto ReloadCheck

    CalledReloadFromPunching: // The gun is down, and lifting it up just to
                              // lower it back down again would look dumb.
        TNT1 A 0 A_TakeInventory("PunchComboCounter",0x7FFFFFFF)
        TNT1 A 0 A_TakeInventory("SlashingLikeAGaijin",1)
        TNT1 A 0 A_ClearRefire
        TNT1 A 0 A_JumpIfInventory("WeebCell",1,1)
        goto PunchReloadNope
        TNT1 A 0
        TNT1 A 0 A_JumpIfInventory("SpecialistMagazine",0,"PunchReloadNope")
        TNT1 A 0 A_PlaySoundEx("special/unload","soundslot7",0,0)
        TNT1 A 4 // Same duration as the one in Reload.
        TNT1 A 6
        goto ReloadLoop
    PunchReloadNope:
        TNT1 A 1 A_ClearRefire
        W2PD ONMLA 1 A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)
        Goto AltFired
    }
}