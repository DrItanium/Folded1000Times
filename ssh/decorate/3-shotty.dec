actor AvengerMagazine : Ammo
{
    Inventory.PickupMessage "Don't think you should've gotten this."
    Inventory.Amount 1
    Inventory.MaxAmount 10
    Ammo.BackpackAmount 0
    Ammo.BackpackMaxAmount 10
    +IGNORESKILL
    Inventory.Icon "ASHLC0"
    States
    {
    Spawn:
        NOPE A 1
        stop
    }
}

