        TNT1 A 0 A_JumpIfInventory("HaggarModeOn",1,"SpecialAttackFinish")
        TNT1 A 0 A_JumpIfInventory("RangedModeOn",1,"SpecialAttackFinish")
        TNT1 A 0 A_JumpIfInventory("TricksterModeOn",1,"GrappleBeam")
        TNT1 A 1
        loop
    GrappleBeam:
        //TNT1 A 0 A_JumpIfInventory("SuperMeterCounter",10,1)
        //goto SpecialAttackFinish
        TNT1 A 0
        TNT1 A 0 A_ClearRefire
        TNT1 A 2 A_FireBullets(0, 0, 1, 0, "GrapplePuff", 0, 1540)//1028)
        TNT1 AA 1 A_JumpIfInventory("GrappleHit",1,"GrappleSound")
        TNT1 A 15 A_PlayWeaponSound("claw/firemiss")
        TNT1 A 0 A_JumpIfInventory("SynthAltFire",1,"GunSpecial")
        TNT1 A 0 A_TakeInventory("PunchComboCounter",0x7FFFFFFF)
        TNT1 A 0 A_TakeInventory("SlashingLikeAGaijin",1)
        TNT1 A 0 A_TakeInventory("ShootingLikeABaka",1)
        TNT1 A 0 A_ClearRefire
        SHF0 DCB 1 A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)
        goto Ready
    GrappleSound:
        TNT1 A 0 A_PlayWeaponSound("claw/firehit")
        TNT1 A 0 ACS_ExecuteAlways(655, 0, ACS_ExecuteWithResult(657, 1), 2, 0)
        TNT1 A 0 A_TakeInventory("GrappleTIDIndicator",1)
        TNT1 A 0 A_TakeInventory("GrappleHit",1)
        //TNT1 A 0 A_TakeInventory("SuperMeterCounter",10)
        TNT1 A 0 A_GiveInventory("Grappling",1)
        TNT1 A 1
        TNT1 A 0 A_TakeInventory("PunchComboCounter",0x7FFFFFFF)
        TNT1 A 0 A_TakeInventory("SlashingLikeAGaijin",1)
        TNT1 A 0 A_TakeInventory("ShootingLikeABaka",1)
        TNT1 A 0 A_ClearRefire
        SHF0 DCB 1 A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)
        goto Ready
    CancelGrapple:
        TNT1 A 0
        TNT1 A 0 A_TakeInventory("Grappling",1)
        TNT1 A 20 A_PlaySoundEx("claw/fireend","soundslot6",0,0)
        goto Ready

    SpecialAttackFinish:
        TNT1 A 0 A_TakeInventory("PunchComboCounter",0x7FFFFFFF)
        TNT1 A 0 A_TakeInventory("KharonSwung",1)
        TNT1 A 0 A_TakeInventory("SlashingLikeAGaijin",1)
        TNT1 A 0 A_TakeInventory("ShootingLikeABaka",1)
        TNT1 A 0 A_TakeInventory("DoubleTapBack", 1)
        TNT1 A 0 A_TakeInventory("DoubleTapLeft", 1)
        TNT1 A 0 A_TakeInventory("DoubleTapRight", 1)
        TNT1 A 0 A_TakeInventory("DoubleTapForward", 1)
        TNT1 A 0 A_ClearRefire
        goto Fired3

    AltFire:
        TNT1 A 0
        TNT1 A 0 A_JumpIfInventory("SlashingLikeAGaijin",1,"AltFirePrepare")
        TNT1 A 0 A_JumpIfInventory("ShootingLikeABaka",1,"AltFireGun")
        TNT1 A 0 A_GiveInventory("ShootingLikeABaka",1)
        goto AltFireGun
    AltFirePrepare:
        TNT1 A 0 A_TakeInventory("SlashingLikeAGaijin",1)
        TNT1 A 0 A_TakeInventory("PunchComboCounter",4)
        TNT1 A 0 A_GiveInventory("ShootingLikeABaka",1)
        W3AV PONMA 1 A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)
    AltFireGun:
        TNT1 A 0 A_JumpIfNoAmmo("Nope")
        TNT1 A 0 A_AlertMonsters
        TNT1 A 0 A_PlaySoundEx("avenger/fire","soundslot6",0)
        TNT1 A 0 A_Light1
        TNT1 A 0 A_JumpIfInventory("RangedModeOn",1,"AltFireGunTracer")
        TNT1 AAAAA 0 A_FireCustomMissile("ShottyBullet1",random(-350,350)/100.00,0,0,1,0,random(-275,275)/100.00)
        TNT1 AAAAA 0 A_FireCustomMissile("ShottyBullet2",random(-250,250)/100.00,0,0,1,0,random(-200,200)/100.00)
        TNT1 AAAA 0 A_FireCustomMissile("ShottyBullet1",random(-200,200)/100.00,0,0,1,0,random(-150,150)/100.00)
        TNT1 AAA 0 A_FireCustomMissile("ShottyBullet2",random(-150,150)/100.00,0,0,1,0,random(-100,100)/100.00)
        TNT1 AA 0 A_FireCustomMissile("ShottyBullet2",random(-100,100)/100.00,0,0,1,0,random(-50,50)/100.00)
        TNT1 A 0 A_FireCustomMissile("ShottyBullet2",random(-50,50)/100.00,1,0,1,0,random(-25,25)/100.00)
        TNT1 A 0 A_JumpIfInventory("GrabbingTheWall",1,2)
        TNT1 A 0 A_Recoil(2.5)
        TNT1 A 0
        TNT1 A 0 A_JumpIfInventory("IAmASillyPersonWhoDoesntLikeRecoil",1,"NoRecoil")
        TNT1 A 0 Radius_Quake(2,3,0,16,0)
        TNT1 A 0 A_SetPitch(pitch-(random(300,600)/100.00))
        TNT1 A 0 A_SetAngle(angle+(random(-250,250)/100.00))
        W3AV B 2 Bright A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH) // 2
        W3AV CCD 1 A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)      // 5
        goto AltFireGunFinish
    AltFireGunTracer:
        TNT1 AAAAA 0 A_FireCustomMissile("ShottyBullet1Tracer",random(-350,350)/100.00,0,0,1,0,random(-275,275)/100.00)
        TNT1 AAAAA 0 A_FireCustomMissile("ShottyBullet2Tracer",random(-250,250)/100.00,0,0,1,0,random(-200,200)/100.00)
        TNT1 AAAA 0 A_FireCustomMissile("ShottyBullet1Tracer",random(-200,200)/100.00,0,0,1,0,random(-150,150)/100.00)
        TNT1 AAA 0 A_FireCustomMissile("ShottyBullet2Tracer",random(-150,150)/100.00,0,0,1,0,random(-100,100)/100.00)
        TNT1 AA 0 A_FireCustomMissile("ShottyBullet2Tracer",random(-100,100)/100.00,0,0,1,0,random(-50,50)/100.00)
        TNT1 A 0 A_FireCustomMissile("ShottyBullet2Tracer",random(-50,50)/100.00,1,0,1,0,random(-25,25)/100.00)
        TNT1 A 0 A_JumpIfInventory("GrabbingTheWall",1,2)
        TNT1 A 0 A_Recoil(2.5)
        TNT1 A 0
        TNT1 A 0 A_JumpIfInventory("IAmASillyPersonWhoDoesntLikeRecoil",1,"NoRecoil")
        TNT1 A 0 Radius_Quake(2,3,0,16,0)
        TNT1 A 0 A_SetPitch(pitch-(random(300,600)/100.00))
        TNT1 A 0 A_SetAngle(angle+(random(-250,250)/100.00))
        W3AV Q 2 Bright A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH) // 2
        W3AV RRD 1 A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)      // 5
    AltFireGunFinish:
    NoRecoil:
        TNT1 A 0 A_Light0
        W3AV E 1 A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)        // 6
        W3AV A 2 A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)        // 8
        W3A2 ABCD 2 A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)     // 16
        W3A2 E 3 A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)        // 19
        TNT1 A 0 A_PlayWeaponSound("avenger/pump")
        W3A2 FGH 2 A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)      // 25
        TNT1 A 0 A_FireCustomMissile("ShellCasingSpawner2",0,0,2)
        W3A2 H 4 A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)        // 29
        W3A2 IJK 2 A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)      // 35
        W3A2 L 3 A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)        // 38
        W3A2 MMNNO 1 A_WeaponReady(WRF_NOSWITCH)                 // Doom shotgun takes 44 tics to fire.
        W3AV A 1 A_WeaponReady(WRF_NOSWITCH)                     // IF ANYONE ELSE SAY THIS WEAPON TOO SLOW THEN BOYYYYY
        Goto AltFired
    Nope:
        TNT1 A 0 A_PlaySoundEx("avenger/dryfire","soundslot7",0,0)
        W3AV A 7 A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)
        goto ReloadCheck
    ReloadCheck:
        TNT1 A 0 A_JumpIfInventory("WeebShell",1,1)
        goto AltFired
        TNT1 A 0
        TNT1 A 0 A_JumpIfInventory("AvengerMagazine",0,"AltFired")
    Reload:
        TNT1 A 0 A_PlaySoundEx("avenger/unload","soundslot6",0,0)
        W3AV MNOP 1 A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)
        TNT1 A 2
        TNT1 A 0 A_PlaySoundEx("avenger/reload","soundslot7",0,0)
        TNT1 A 6
    ReloadLoop:
        TNT1 A 0 A_JumpIfInventory("WeebShell",1,1)
        goto ReloadFinish
        TNT1 A 0
        TNT1 A 0 A_TakeInventory("WeebShell",1)
        TNT1 A 0 A_GiveInventory("AvengerMagazine",1)
        TNT1 A 0 A_JumpIfInventory("AvengerMagazine",10,"ReloadFinish")
        goto ReloadLoop
    ReloadFinish:
        TNT1 A 0 A_TakeInventory("ShootingLikeABaka",1)
        TNT1 A 1 A_ClearRefire
        W3AV PONMA 1 A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)
        Goto AltFired
    AltFired:
        TNT1 A 0 A_TakeInventory("PunchComboCounter",7)
        TNT1 A 0 A_TakeInventory("SlashingLikeAGaijin",1)
        TNT1 A 0 A_TakeInventory("ShootingLikeABaka",1)
        TNT1 A 0 A_ClearRefire
        W3AV A 1 A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)
        Goto Ready

    CalledReloadFromPunching: // The gun is down, and lifting it up just to
                              // lower it back down again would look dumb.
        TNT1 A 0 A_TakeInventory("PunchComboCounter",0x7FFFFFFF)
        TNT1 A 0 A_TakeInventory("SlashingLikeAGaijin",1)
        TNT1 A 0 A_ClearRefire
        TNT1 A 0 A_JumpIfInventory("WeebShell",1,1)
        goto PunchReloadNope
        TNT1 A 0
        TNT1 A 0 A_JumpIfInventory("AvengerMagazine",0,"PunchReloadNope")
        TNT1 A 0 A_PlaySoundEx("avenger/unload","soundslot6",0,0)
        TNT1 A 4 // Same duration as the one in Reload.
        TNT1 A 2
        TNT1 A 0 A_PlaySoundEx("avenger/reload","soundslot7",0,0)
        TNT1 A 6
        goto ReloadLoop
    PunchReloadNope:
        TNT1 A 1 A_ClearRefire
        W3AV PONMA 1 A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)
        Goto AltFired
    }
}

actor ShellCasingSpawner2 : FastProjectile
{
    Radius 6
    Height 6
    Projectile
    Damage 0
    Speed 5
    SeeSound "none"
    DeathSound "none"
    Decal "none"
    +CLIENTSIDEONLY
    States
    {
      Spawn:
        TNT1 A 1
        TNT1 A 1 A_SpawnItemEx("DukeCasing2",0,4,-1,random(1,3),0,random(-1,1),random(85,90),0)
        Stop
    }
}

actor DukeCasing2
{
    Speed 1
    Scale 0.07
    Height 1
    Radius 0
    Gravity 0.8
    -SOLID
    +MISSILE
    +MOVEWITHSECTOR
    +CLIENTSIDEONLY
    +NOBLOCKMAP
    +NOTELEPORT
    States
    {
      Spawn:
        SHTX A -1 BRIGHT
        Loop

      Death:
        SHTX B 0
        Stop
    }
}