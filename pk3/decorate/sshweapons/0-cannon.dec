    FireForReal:
        MSTO F 1 A_WeaponReady(WRF_NOFIRE | WRF_NOSWITCH)
        TNT1 A 0 Radius_Quake(3,10,0,16,0)
        TNT1 A 0 A_SetBlend("Green",0.1,20)
        TNT1 A 0 A_PlayWeaponSound("mastodon/fire")
        TNT1 A 0 A_AlertMonsters
