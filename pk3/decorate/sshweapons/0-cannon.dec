        TNT1 A 0 A_StopSoundEx("soundslot7")
        TNT1 A 0 A_PlaySoundEx("silence","soundslot7",0,0)
        TNT1 A 0 A_PlaySoundEx("mastodon/idledown","soundslot7",0,0)
        TNT1 A 0 A_PlaySoundEx("mastodon/lower","soundslot6",0,0)
