        TNT1 A 0
        TNT1 A 0
        TNT1 A 12
        TNT1 A 0 A_Jump(255,"SpawnMedikit","SpawnStimpack","SpawnBullets","SpawnShells","SpawnRockets","SpawnCell","SpawnSoul")
