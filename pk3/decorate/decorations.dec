        Goto Spawn
    SpawnSoul:
        TNT1 A 0
        TNT1 A 0 A_FaceTarget
        TNT1 A 0 A_SpawnItemEx("TinyMeterPickup7",0,0,0,random(40,80),0,random(1,20),0,SXF_NOCHECKPOSITION)
        Goto Spawn
    Nothing:
        TNT1 A 0
        Goto Spawn
    }
}