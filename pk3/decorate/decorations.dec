actor DecorationShooterTurbo
{
    Monster
    -SOLID
    +THRUACTORS
    +ALWAYSFAST
    +LOOKALLAROUND
    Health -1
    RenderStyle None
    Speed 0
    States
    {
    Spawn:
        TNT1 A 0
        TNT1 A 0
        TNT1 A 0 A_ClearTarget
        TNT1 AAAAAAAAAAAA 1 A_Look
        Loop
    See:
