actor DecorationSpawner
{
    Monster
    -SOLID
    +THRUACTORS
    +ALWAYSFAST
    +LOOKALLAROUND
    Health -1
    RenderStyle None
    States
    {
    Shoot:
        TNT1 A 0
        TNT1 A 0
        TNT1 A 0
        TNT1 AAAAAAAAAA 1 A_LookEx(0,0,500)
        Goto DunSee
    See:
        TNT1 A 0 A_Jump(255,"ShootMedikit","ShootStimpack","ShootBullets","ShootShells","ShootRockets","ShootCell","ShootSoul","Nothing","Nothing","Nothing","Nothing","Nothing","Nothing","Nothing","Nothing")
        TNT1 A 1
        loop
    ShootStimpack:
        TNT1 A 0
        TNT1 A 0 A_FaceTarget
        TNT1 A 0 A_SpawnItemEx("Stimpack",0,0,0,random(8-16),0,random(1,3),0,SXF_NOCHECKPOSITION)
        stop
    ShootMedikit:
        TNT1 A 0
        TNT1 A 0 A_FaceTarget
        TNT1 A 0 A_SpawnItemEx("Medikit",0,0,0,random(8-16),0,random(1,3),0,SXF_NOCHECKPOSITION)
        stop
    ShootBullets:
        TNT1 A 0
        TNT1 A 0 A_FaceTarget
        TNT1 A 0 A_SpawnItemEx("WeebClip",0,0,0,random(8-16),0,random(1,3),0,SXF_NOCHECKPOSITION)
        stop
    ShootShells:
        TNT1 A 0
        TNT1 A 0 A_FaceTarget
        TNT1 A 0 A_SpawnItemEx("WeebShell",0,0,0,random(8-16),0,random(1,3),0,SXF_NOCHECKPOSITION)
        stop
    ShootRockets:
        TNT1 A 0
        TNT1 A 0 A_FaceTarget
        TNT1 A 0 A_SpawnItemEx("WeebRocket",0,0,0,random(8-16),0,random(1,3),0,SXF_NOCHECKPOSITION)
        stop
    ShootCell:
        TNT1 A 0
        TNT1 A 0 A_FaceTarget
        TNT1 A 0 A_SpawnItemEx("WeebCell",0,0,0,random(8-16),0,random(1,3),0,SXF_NOCHECKPOSITION)
        stop
    ShootSoul:
        TNT1 A 0
        TNT1 A 0 A_FaceTarget
        TNT1 A 0 A_SpawnItemEx("TinyMeterPickup7",0,0,0,random(8-16),0,random(1,3),0,SXF_NOCHECKPOSITION)
        stop
    Nothing:
        TNT1 A 0
        stop
    DunSee:
        TNT1 A 0 A_Jump(255,"SpawnMedikit","SpawnStimpack","SpawnBullets","SpawnShells","SpawnRockets","SpawnCell","SpawnSoul","Nothing","Nothing","Nothing","Nothing","Nothing","Nothing","Nothing","Nothing")
        TNT1 A 1
        loop
    SpawnStimpack:
        TNT1 A 0
        TNT1 A 0 A_SpawnItemEx("Stimpack",0,0,0,random(-400,400)/100.00,random(-400,400)/100.00,random(200,600)/100.00,random(-180,180),32)
        stop
    SpawnMedikit:
        TNT1 A 0
        TNT1 A 0 A_SpawnItemEx("Medikit",0,0,0,random(-400,400)/100.00,random(-400,400)/100.00,random(200,600)/100.00,random(-180,180),32)
        stop
    SpawnBullets:
        TNT1 A 0
        TNT1 A 0 A_SpawnItemEx("WeebClip",0,0,0,random(-400,400)/100.00,random(-400,400)/100.00,random(200,600)/100.00,random(-180,180),32)
        stop
    SpawnShells:
        TNT1 A 0
        TNT1 A 0 A_SpawnItemEx("WeebShell",0,0,0,random(-400,400)/100.00,random(-400,400)/100.00,random(200,600)/100.00,random(-180,180),32)
        stop
    SpawnRockets:
        TNT1 A 0
        TNT1 A 0 A_SpawnItemEx("WeebRocket",0,0,0,random(-400,400)/100.00,random(-400,400)/100.00,random(200,600)/100.00,random(-180,180),32)
        stop
    SpawnCell:
        TNT1 A 0
        TNT1 A 0 A_SpawnItemEx("WeebCell",0,0,0,random(-400,400)/100.00,random(-400,400)/100.00,random(200,600)/100.00,random(-180,180),32)
        stop
    SpawnSoul:
        TNT1 A 0
        TNT1 A 0 A_SpawnItemEx("TinyMeterPickup7",0,0,0,random(-400,400)/100.00,random(-400,400)/100.00,random(200,600)/100.00,random(-180,180),32)
        stop
    }
}

ACTOR DestructableDecorationBase
{
    Radius 1
    Height 1
    Health 0x7FFFFFFF
    Mass 0x7FFFFFFF
    ProjectilePassHeight -16
    +SOLID
    +SHOOTABLE
    +NORADIUSDMG
    +RANDOMIZE
    +NOBLOOD
    +DONTGIB
    +FLOORCLIP
    BloodType "BlankPuff"
    RadiusDamageFactor 0.0
    DamageFactor "Normal", 0.0
    DamageFactor "Kubiuti", 1.0
    DamageFactor "HaltHammerzeitImpact", 1.0
    DamageFactor "IronFistDamage", 1.0
    PainChance 0
    PainChance "Kubiuti", 255
    PainChance "HaltHammerzeitImpact", 255
    PainChance "IronFistDamage", 255
}

