{
    PROJECTILE
    +LOWGRAVITY
    -NOGRAVITY
    -MISSILE
    Speed 0
    Health 1
    Radius 1
    Height 2
    Mass 1000
    States
    {
    Spawn:
        XDE2 BBBBBBBBB 5 //A_CustomMissile("WBsmokeMin",random(1,8),random(-8,8)) //A_SpawnItem("WBsmokeMin",0)
        XDE2 B 0 A_FadeOut
        Loop
    }
}

actor HEADKubiuted : POSXKubiuted
{
    +SOLID
    Radius 16
    Height 33
    MeleeRange 40
    BloodColor "00 88 FF"
    States
    {
    Spawn:
        XDE1 A 0
        XDE1 A 0 ThrustThing (random(0,255),random(1,4))
        XDE1 A 0 ThrustThingZ(0,random(10,30),0,0)
        XDE1 AAA 1 A_SpawnItem("BloodShowerB3",0)
        XDE1 B 0 A_CheckFloor("Spawned")
        XDE1 B 0 A_SpawnDebris("BloodShowerB2")
        XDE1 BBB 1 A_SpawnItem("BloodShowerB3",0)
        XDE1 C 0 A_CheckFloor("Spawned")
        XDE1 C 0 A_SpawnDebris("BloodShowerB2")
        XDE1 CCC 1 A_SpawnItem("BloodShowerB3",0)
        XDE1 C 0 A_CheckFloor("Spawned")
        XDE1 D 0 A_SpawnDebris("BloodShowerB2")
        XDE1 DDD 1 A_SpawnItem("BloodShowerB3",0)
        XDE1 D 0 A_CheckFloor("Spawned")
        XDE1 E 0 A_SpawnDebris("BloodShowerB2")
        XDE1 EEE 1 A_SpawnItem("BloodShowerB3",0)
        XDE1 E 0 A_CheckFloor("Spawned")
        XDE1 F 0 A_SpawnDebris("BloodShowerB2")
        XDE1 FFF 1 A_SpawnItem("BloodShowerB3",0)
        XDE1 F 0 A_CheckFloor("Spawned")
        XDE1 G 0 A_SpawnDebris("BloodShowerB2")
        XDE1 GGG 1 A_SpawnItem("BloodShowerB3",0)
        XDE1 G 0 A_CheckFloor("Spawned")
        XDE1 H 0 A_SpawnDebris("BloodShowerB2")
        XDE1 HHH 1 A_SpawnItem("BloodShowerB3",0)
        XDE1 H 0 A_CheckFloor("Spawned")
        goto Spawn+3
    Spawned:
        XDE1 A 0 A_Jump(200,3)
        XDE1 A 1 A_PlaySound("S_Blodd")
        XDE1 A 0 ThrustThingZ(0,random(10,20),0,0)
        goto Spawn+3
        XDE1 A 0 A_PlaySound("S_Blodd")
        XDE1 A 0 A_ChangeFlag("PUSHABLE",0)
    Spawned2:
        XDE1 A 0 A_Jump(160,2)
        XDE1 A 1 A_chase
        goto Spawned2+1
        XDE1 E 1 A_chase
        goto Spawned2+2
    Melee:
        XDE1 A 1 A_FaceTarget
        XDE1 A 0 damageThing(1)
        XDE1 A 0 A_ChangeFlag("PUSHABLE",1)
        XDE1 A 0 ThrustThingZ(0,random(15,40),0,0)
        XDE1 A 0 A_Recoil(2+random(0,8))
        goto Spawn+3
    Death:
        BLD1 C 0 A_Fall
        BLD1 C 0 A_Scream
        BLD1 CCC 0 A_CustomMissile("EnemySplashGus2",0,0,random(0,360),2,random(0,360))
        BLD1 CCCCC 0 A_SpawnDebris("BloodShowerB2")
        BLD1 CBA 1
        Stop
    }
}