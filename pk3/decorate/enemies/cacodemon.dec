ACTOR WeeabooCacodemon : Cacodemon replaces Cacodemon
{
    Speed 10
    Species "Body"
    PainChance "kubiuti", 255
    PainChance "KubiSpinAttack", 255
    PainChance "KubiLungeAttack", 255
    PainChance "HaltHammerzeitImpact", 255
    DamageFactor "GuardMove", 0.0
    DamageFactor "EnemyInfightDamageCaco", 0.0
    DamageFactor "ArchvileFire", 0.0
    DamageFactor "ExodusJump", 0.0
    BloodColor blue
    +MISSILEMORE
    +MISSILEEVENMORE
    States
    {
    See:
        HEAD A 2 A_FastChase
        HEAD AAAAAAAA 2 A_Chase
        Loop
    Melee:
    Missile:
        HEAD BC 6 A_FaceTarget
        HEAD D 4 Bright A_CustomMissile("CacodemonBall")//A_HeadAttack
        TNT1 A 0 A_Jump(128,"See")
        TNT1 A 0 A_SpidRefire
        Goto See

    Pain.KubiLungeAttack:
    Pain.KubiSpinAttack:
        TNT1 A 0 A_PlaySoundEx("sword/hit","soundslot5",0,0)
        TNT1 A 0 A_GiveToTarget("HyperComboCounter",1)
        goto Super::Pain
    Death.KubiLungeAttack:
    Death.KubiSpinAttack:
        TNT1 A 0 A_PlaySoundEx("sword/hit","soundslot5",0,0)
        TNT1 A 0 A_GiveToTarget("HyperComboCounter",2)
        TNT1 A 0 A_GiveToTarget("SuperMeterCounter",1)
        TNT1 A 0 A_GiveToTarget("MidCombat",75)
        goto death.kubiuti+1

    XDeath:    
    death.rocket:
        HEAD G 0 A_XScream
        HEAD GGGGG 0 A_ThrowGrenade ("BloodShowerB2",-2,5+random(0,7),4)    
        HEAD G 1 A_ThrowGrenade ("BloodShowerB2",-2,5+random(0,7),4)    
        XDE2 A 0 A_CustomMissile("BloodShowerB2",16,0,0)
        XDE2 A 0 A_CustomMissile("BloodShowerB2",16,0,180)
        XDE2 A 0 A_CustomMissile("BloodShowerB2",16,0,30)
        XDE2 A 0 A_CustomMissile("BloodShowerB2",16,0,60)
        XDE2 A 0 A_CustomMissile("BloodShowerB2",16,0,60)
        XDE2 A 0 A_CustomMissile("BloodShowerB2",16,0,90)
        XDE2 A 0 A_CustomMissile("BloodShowerB2",16,0,120)
        XDE2 A 0 A_CustomMissile("BloodShowerB2",16,0,150)
        XDE2 A 0 A_CustomMissile("BloodShowerB2",16,0,-30)
        XDE2 A 0 A_CustomMissile("BloodShowerB2",16,0,-60)
        XDE2 A 0 A_CustomMissile("BloodShowerB2",16,0,-60)
        XDE2 A 0 A_CustomMissile("BloodShowerB2",16,0,-90)
        XDE2 A 0 A_CustomMissile("BloodShowerB2",16,0,-120)
        XDE2 A 0 A_CustomMissile("BloodShowerB2",16,0,-150)
        XDE2 A 1 A_NoBlocking
        XDE2 AAAAAAAAAA 0 A_SpawnDebris("BloodShower2",1)
        XDE2 AAAAAAAAAA 0 A_CustomMissile("EnemySplashGus2",random(24,48),0,random(0,360),2,random(0,360))
        XDE2 A -1 A_SpawnDebris("HEADXDeathed",1)    
        stop
    death.hunter:    
        HEAD G 0 ThrustThing (random(0,255),random(4,8))
        HEAD G 0 ThrustThingZ(0,random(20,60),0,0)
        goto xdeath
    death.kubiuti:
        TNT1 A 0 A_GiveToTarget("SuperMeterCounter",2)
        HEAD G 0 A_PlaySound("enemy/bisected")
        HEAD G 0 A_Jump(64,"death.hunter")
        HEAD G 0 A_Jump(128,"kubiuti2")
        HEAD GG 1 A_ThrowGrenade("BloodSplashGus1b",-2,5+random(0,7),40)    
        HEAD H 2 A_NoBlocking
        HEAD I 2
        XDE2 GGGGGGGGGG 0 A_ThrowGrenade ("BloodSplashGus1b",-2,5+random(0,7),40)    
        XDE2 A -1 A_SpawnItem("HEADKubiuted",0,32)
        stop    
    kubiuti2:
        XDE1 IIIII 1 A_ThrowGrenade ("BloodSplashGus1b",-2,5+random(0,7),40)    
        XDE1 J 0 A_NoBlocking
        XDE1 JJJJJ 1 A_ThrowGrenade ("BloodSplashGus1b",-2,5+random(0,7),40)    
        XDE1 KKKKK 1 A_ThrowGrenade ("BloodSplashGus1b",-2,5+random(0,7),40)    
        XDE1 LLLLL 1 A_ThrowGrenade ("BloodSplashGus1b",-2,5+random(0,7),40)    
        XDE1 MMMMM 1 A_ThrowGrenade ("BloodSplashGus1b",-2,5+random(0,7),40)    
        XDE1 NNNNN 1 A_ThrowGrenade ("BloodSplashGus1b",-2,5+random(0,7),40)    
        XDE1 OOOOO 1 A_ThrowGrenade ("BloodSplashGus1b",-2,5+random(0,7),40)    
        XDE1 O -1 A_SpawnDebris("BloodShowerB2",1)    
        stop
    }
}

actor CacodemonBall2 : CacodemonBall replaces CacodemonBall
{
    Health 5
    Speed 15
    FastSpeed 25
    Species "EnemyShot"
    DamageFactor "EnemyInfightDamage", 0.0
    DamageFactor "EnemyInfightDamageImp", 0.0
    DamageFactor "EnemyInfightDamageArachnotron", 0.0
    DamageFactor "EnemyInfightDamageNoble", 0.0
    DamageFactor "EnemyInfightDamageRevenant", 0.0
    DamageFactor "EnemyInfightDamageFatso", 0.0
    DamageFactor "EnemyInfightDamageCaco", 0.0
    DamageFactor "ArchvileFire", 0.0
    DamageFactor "HaltHammerzeit", 0.0
    DamageFactor "ExodusJump", 0.0
    +THRUSPECIES
    +SHOOTABLE
    +THRUGHOST
    -NOBLOCKMAP 
    +NOBLOOD
    States
    {
    Death:
        TNT1 A 0 A_UnsetShootable
        TNT1 A 0 A_SpawnItemEx("EnemyCacoInfight",0,0,0,0,0,0,0,32)
        goto Super::Death
    }
}

