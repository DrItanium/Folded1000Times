    {
    Spawn:
        CLNK AB 10 A_Look
        Loop
    See:
        TNT1 A 0 A_JumpIfCloser(384,"SeeClose")
        TNT1 A 0 A_PlaySoundEx("steelbeast/step","soundslot7",0,0)
        CLNK AA 2 A_FastChase
        CLNK B 4 A_Chase
        TNT1 A 0 A_PlaySoundEx("steelbeast/step","soundslot7",0,0)
        CLNK CC 2 A_FastChase
        CLNK D 4 A_Chase
