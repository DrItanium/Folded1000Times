ACTOR WeeabooDemon : Demon replaces Demon
{
    Speed 10
    Species "Body"
    PainChance "kubiuti", 255
    PainChance "KubiSpinAttack", 255
    PainChance "KubiLungeAttack", 255
    PainChance "HaltHammerzeitImpact", 255
    DamageFactor "GuardMove", 0.0
    DamageFactor "ArchvileFire", 0.0
    DamageFactor "ExodusJump", 0.0
    States
    {
    See: // Apparently increasing the speed gets the critter stuck on some parts of the environment.
        SARG A 2 A_Chase
        SARG AA 1 A_Chase
        SARG B 2 A_Chase
        SARG BB 1 A_Chase
        SARG C 2 A_Chase
        SARG CC 1 A_Chase
        SARG D 2 A_Chase
        SARG DD 1 A_Chase
        Loop
    Melee:
        SARG E 6 A_FaceTarget
        SARG F 6 A_SargAttack
        SARG G 6 A_SargAttack
        Goto See
    Death:
        TNT1 A 0 A_Jump(96,"Death2","Death3")
        goto "Super::Death"

    Pain.KubiLungeAttack:
    Pain.KubiSpinAttack:
        TNT1 A 0 A_PlaySoundEx("sword/hit","soundslot5",0,0)
        TNT1 A 0 A_GiveToTarget("HyperComboCounter",1)
        goto Pain
    Death.KubiLungeAttack:
    Death.KubiSpinAttack:
        TNT1 A 0 A_PlaySoundEx("sword/hit","soundslot5",0,0)
        TNT1 A 0 A_GiveToTarget("HyperComboCounter",2)
        //TNT1 A 0 A_GiveToTarget("SuperMeterCounter",1)
        TNT1 A 0 A_GiveToTarget("MidCombat",75)
        goto death.kubiuti+2

    Death2:
        TNT1 A 0
        XDD2 A 5 A_NoBlocking
        XDD2 B 5 A_Scream
        XDD2 CD 5
        XDD2 E -1
        Stop
    Death3:
        TNT1 A 0
        XDD2 F 5 A_NoBlocking
        XDD2 G 5 A_Scream
        XDD2 HI 5
        XDD2 J -1
        Stop
    XDeath:
        SARG P 0 A_SpawnItem("SARXKubiuted",0,40,0)
        SARG PPPPP 0 A_CustomMissile("BloodSplashGus1",random(0,32),0,random(0,360),2,random(0,360))
        SARG PPPPP 0 A_CustomMissile("EnemySplashGus1",random(0,32),0,random(0,360),2,random(0,360))
        SARG P 0 A_XScream
        SARG PPPPP 0 A_CustomMissile("BloodShower5",random(0,32),0,random(0,360),2,random(0,360))
        SARG PPPPP 1 A_CustomMissile("BloodSplashGus1",random(0,32),0,random(0,360),2,random(0,360))
        SARG Q 0 A_NoBlocking
        SARG QQQQQ 1 A_SpawnDebris("BloodShower2")
        SARG RSTU 5
        SARG V -1
        wait

    death.hunter:    
        XDD1 A 0 A_PlaySound("S_Cutdead")
        goto death.kubiuti+4
    death.kubiuti:
        TNT1 A 0 A_AlertMonsters
        TNT1 A 0 A_GiveToTarget("SuperMeterCounter",2)
        XDC1 A 0 A_Jump(128,"Death.Bloody","Death.Hunter","CleaveThrough","CleaveThrough2")
        XDD1 A 0 A_PlaySound("enemy/bisected")
        XDD1 A 0 A_Jump(96,"kubiuti2")
        XDD1 A 0 A_Jump(96,"kubiuti3")
        XDD1 A 10 A_SpawnItem("SARXKubiuted",0,40,0)
        XDD1 B 7 A_NoBlocking
        XDD1 CDE 7
        XDD1 F -1
        wait
    kubiuti2:
        XDD1 G 1 A_SpawnDebris("BloodShower2",0)
        XDD1 GGGGGG 0 A_CustomMissile("EnemySplashGus1",random(32,48),0,random(0,360),2,random(0,360))
        XDD1 GGGGGGGGG 1 A_ThrowGrenade ("BloodShower",-2,5+random(0,7),8)    
        XDD1 H 0 A_NoBlocking
        XDD1 HHHIIIJJJ 2 A_SpawnDebris("BloodSplashGus1",0)
        XDD1 J -1
        wait    
    kubiuti3:
        XDD1 M 10 A_SpawnItem("SARXKubiuted",0,40,0)
        XDD1 N 7 A_NoBlocking
        XDD1 OPQ 7
        XDD1 R -1
        wait

    death.Bloody:
        XDD3 A 0 A_Scream
        XDD3 A 0 A_Jump(96,"XDeath")
        XDD3 A 0 A_Jump(96,"Death.rocket")
        XDD3 A 0 A_Jump(96,"Bloody2")
        XDD3 A 0 A_Jump(96,"Bloody3")
        XDD3 A 0 A_Jump(96,"Bloody4")
        XDD3 AAA 2 A_ThrowGrenade ("BloodSplashGus1",2,5+random(0,7),8)    
        XDD3 B 0 A_Fall
        XDD3 BBB 2 A_ThrowGrenade ("BloodSplashGus1",2,5+random(0,7),8)    
        XDD3 CCC 2 A_ThrowGrenade ("BloodShower",-2,5+random(0,7),8)    
        XDD3 DDDDDD 0 A_CustomMissile("EnemySplashGus1",random(0,32),0,random(0,360),2,random(0,360))
        XDD3 DDDEEE 2 A_ThrowGrenade ("BloodShower",-2,5+random(0,7),8)    
        XDD3 FFFFF 2 A_SpawnDebris("BloodShower2",0)
        XDD3 F -1
        wait
    Bloody2:
        XDD3 GGG 2 A_ThrowGrenade ("BloodSplashGus1",2,5+random(0,7),8)    
        XDD3 H 0 A_Fall
        XDD3 HHH 2 A_ThrowGrenade ("BloodSplashGus1",2,5+random(0,7),8)    
        XDD3 III 2 A_ThrowGrenade ("BloodShower",-2,5+random(0,7),8)    
        XDD3 JJJJJJ 0 A_CustomMissile("EnemySplashGus1",random(0,32),0,random(0,360),2,random(0,360))
        XDD3 JJJKKK 2 A_ThrowGrenade ("BloodShower",-2,5+random(0,7),8)    
        XDD3 LLLLL 2 A_SpawnDebris("BloodShower2",0)
        XDD3 L -1
        wait
    Bloody3:
        XDD4 AAA 2 A_ThrowGrenade ("BloodSplashGus1",2,5+random(0,7),8)    
        XDD4 B 0 A_Fall
        XDD4 BBB 2 A_ThrowGrenade ("BloodSplashGus1",2,5+random(0,7),8)    
        XDD4 CCCCCC 0 A_CustomMissile("EnemySplashGus1",random(0,32),0,random(0,360),2,random(0,360))
        XDD4 CCCDDDEEE 2 A_ThrowGrenade ("BloodShower",-2,5+random(0,7),8)    
        XDD4 FFFFF 2 A_SpawnDebris("BloodShower2",0)
        XDD4 F -1
        wait
    Bloody4:
        XDD4 GGG 2 A_ThrowGrenade ("BloodSplashGus1",2,5+random(0,7),8)    
        XDD4 H 0 A_Fall
        XDD4 HHH 2 A_ThrowGrenade ("BloodSplashGus1",2,5+random(0,7),8)    
        XDD4 IIIIII 0 A_CustomMissile("EnemySplashGus1",random(0,32),0,random(0,360),2,random(0,360))
        XDD4 IIIJJJKKK 2 A_ThrowGrenade ("BloodShower",-2,5+random(0,7),8)    
        XDD4 LLLLL 2 A_SpawnDebris("BloodShower2",0)
        XDD4 L -1
        wait

    CleaveThrough:
        PXD1 A 3
        TNT1 AAAAA 0 A_CustomMissile("BloodSplashGus1",random(0,32),0,random(0,360),2,random(0,360))
        TNT1 AAAAAA 0 A_CustomMissile("BloodShower2",random(0,32),0,random(0,360),2,random(0,360))
        TNT1 A 0 A_XScream
        TNT1 AAAAAA 0 A_CustomMissile("BloodShower2",random(0,32),0,random(0,360),2,random(0,360))
        PXD1 BBBBB 1 A_CustomMissile("BloodSplashGus1",random(0,32),0,random(0,360),2,random(0,360))
        TNT1 A 0 A_NoBlocking
        TNT1 AAAAAA 0 A_CustomMissile("BloodShower2",random(0,32),0,random(0,360),2,random(0,360))
        TNT1 AAAAA 0 A_CustomMissile("BloodShower5",random(0,32),0,random(0,360),2,random(0,360))
        PXD1 CCCCC 1 A_CustomMissile("BloodShower5",random(0,32),0,random(0,360),2,random(0,360))
        TNT1 AAAAA 0 A_CustomMissile("BloodShower5",random(0,32),0,random(0,360),2,random(0,360))
        TNT1 AAAAAA 0 A_CustomMissile("BloodShower2",random(0,32),0,random(0,360),2,random(0,360))
        PXD1 DE 4
        TNT1 AAAAAA 0 A_CustomMissile("BloodShower2",random(0,32),0,random(0,360),2,random(0,360))
        PXD1 F 3
        TNT1 AAAAAA 0 A_CustomMissile("BloodShower2",random(0,32),0,random(0,360),2,random(0,360))
        PXD1 G -1
        stop
    CleaveThrough2:
        PXD1 H 3
        TNT1 AAAAA 0 A_CustomMissile("BloodSplashGus1",random(0,32),0,random(0,360),2,random(0,360))
        TNT1 AAAAAA 0 A_CustomMissile("BloodShower2",random(0,32),0,random(0,360),2,random(0,360))
        TNT1 A 0 A_XScream
        TNT1 AAAAAA 0 A_CustomMissile("BloodShower2",random(0,32),0,random(0,360),2,random(0,360))
        PXD1 IIIII 1 A_CustomMissile("BloodSplashGus1",random(0,32),0,random(0,360),2,random(0,360))
        TNT1 A 0 A_NoBlocking
        TNT1 AAAAAA 0 A_CustomMissile("BloodShower2",random(0,32),0,random(0,360),2,random(0,360))
        TNT1 AAAAA 0 A_CustomMissile("BloodShower5",random(0,32),0,random(0,360),2,random(0,360))
        PXD1 JJJJJ 1 A_CustomMissile("BloodShower5",random(0,32),0,random(0,360),2,random(0,360))
        TNT1 AAAAA 0 A_CustomMissile("BloodShower5",random(0,32),0,random(0,360),2,random(0,360))
        TNT1 AAAAAA 0 A_CustomMissile("BloodShower2",random(0,32),0,random(0,360),2,random(0,360))
        PXD1 KL 4
        TNT1 AAAAAA 0 A_CustomMissile("BloodShower2",random(0,32),0,random(0,360),2,random(0,360))
        PXD1 M 3
        TNT1 AAAAAA 0 A_CustomMissile("BloodShower2",random(0,32),0,random(0,360),2,random(0,360))
        PXD1 N -1
        stop
    }
}

