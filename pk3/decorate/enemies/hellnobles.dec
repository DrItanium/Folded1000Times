    death.kubiuti:
        TNT1 A 0 A_GiveToTarget("SuperMeterCounter",5)
        BOSS I 1
        TNT1 A 0 A_Jump(128,"Death.Bloody","Death.Hunter")
        BOSS I 0 A_Fall
        XDI1 A 0 A_SpawnItem("BOSSKubiuted",0,56,0)
