actor WeeabooBaronBall : BaronBall replaces BaronBall
{
    Radius 10
    Height 16
    Health 5
    Speed 17
    FastSpeed 22
    Species "EnemyShot"
    DamageFactor "EnemyInfightDamage", 0.0
    DamageFactor "EnemyInfightDamageImp", 0.0
    DamageFactor "EnemyInfightDamageArachnotron", 0.0
    DamageFactor "EnemyInfightDamageNoble", 0.0
    DamageFactor "EnemyInfightDamageRevenant", 0.0
    DamageFactor "EnemyInfightDamageFatso", 0.0
    DamageFactor "EnemyInfightDamageCaco", 0.0
    DamageFactor "ArchvileFire", 0.0
    DamageFactor "HaltHammerzeit", 0.0
    DamageFactor "ExodusJump", 0.0
    +THRUSPECIES
    +SHOOTABLE
    +THRUGHOST
    -NOBLOCKMAP 
    +NOBLOOD
    States
    {
    Death:
        TNT1 A 0 A_UnsetShootable
        TNT1 A 0 A_SpawnItemEx("EnemyNobleInfight",0,0,0,0,0,0,0,32)
        goto Super::Death
    }
}

actor BOSSKubiuted : POSXKubiuted
{
    BloodColor "77 EE 00" 
    States
    {
    Spawn:
        XDI1 D 0
        XDI1 D 0 ThrustThing (random(0,255),random(1,4))
        XDI1 D 0 ThrustThingZ(0,random(15,40),0,0)
        XDI1 D 1 A_SpawnItem("BloodShowerC3",0)
        XDI1 D 0 A_CheckFloor("Spawned")
        goto Spawn+3
    Spawned:
        XDI1 E 0 A_PlaySound("S_Blodd")
        XDI1 E 0 A_ChangeFlag("PUSHABLE",0)
    Spawned2:
        XDI1 E 1 A_Look
        goto Spawned2
    See:    
        XDI1 E 1 A_Chase
        loop
    Melee:
        XDI1 D 1 A_FaceTarget
        XDI1 D 0 damageThing(1)
        XDI1 D 0 A_ChangeFlag("PUSHABLE",1)
        XDI1 D 0 ThrustThingZ(0,random(15,40),0,0)
        XDI1 D 0 A_Recoil(2+random(0,8))
        goto Spawn+3
    Death:
        BLD1 C 0 A_Fall
        BLD1 C 0 A_Scream
        BLD1 CCCCC 0 A_SpawnDebris("BloodShowerC2")
        BLD1 CBA 1
        Stop
    }
}

