ACTOR WeeabooHellKnight : HellKnight replaces HellKnight
{
    Species "Body"
    PainChance "kubiuti", 255
    PainChance "KubiSpinAttack", 255
    PainChance "KubiLungeAttack", 255
    PainChance "HaltHammerzeitImpact", 255
    DamageFactor "GuardMove", 0.0
    DamageFactor "EnemyInfightDamageNoble", 0.0
    DamageFactor "ArchvileFire", 0.0
    DamageFactor "ExodusJump", 0.0
    BloodColor DarkOliveGreen
    +MISSILEMORE
    States
    {
    Melee:
    Missile:
        BOS2 EF 9 A_FaceTarget
        BOS2 G 8 A_CustomMissile("WeeabooBaronBall")//A_BruisAttack
        TNT1 A 0 A_Jump(128,"See")
        TNT1 A 0 A_SpidRefire
        Goto See

    Pain.KubiLungeAttack:
    Pain.KubiSpinAttack:
        TNT1 A 0 A_PlaySoundEx("sword/hit","soundslot5",0,0)
        TNT1 A 0 A_GiveToTarget("HyperComboCounter",1)
        goto Super::Pain
    Death.KubiLungeAttack:
    Death.KubiSpinAttack:
        TNT1 A 0 A_GiveToTarget("SuperMeterCounter",1)
        TNT1 A 0 A_GiveToTarget("HyperComboCounter",2)
        TNT1 A 0 A_GiveToTarget("MidCombat",75)
        TNT1 A 0 A_PlaySoundEx("sword/hit","soundslot5",0,0)
        goto death.kubiuti+1

    Death:
        BOS2 I 8
        BOS2 J  8 A_Scream
        BOS2 K  8
        BOS2 L  8 A_NoBlocking
        BOS2 MN 8
        BOS2 O -1
        Stop
    XDeath:
        BOS2 P  5 
        BOS2 Q  0 A_Scream
        BOS2 QQQQQ 1 A_CustomMissile("EnemySplashGus3",random(24,32),0,random(0,360),2,random(0,360))
        BOS2 R  5
        BOS2 S  5 A_NoBlocking
        BOS2 TUVW 5
        BOS2 X -1 A_BossDeath   
        Stop    

    Death.Bloody2:
        BOS2 H 2 A_NoBlocking
        XDI3 A 0 A_PlaySound("S_Cutdead")
        XDI3 AAAAA 2 A_ThrowGrenade ("BloodSplashGus1c",4,5+random(0,7),8)
        XDI3 AAABBCC 2 A_SpawnDebris("BloodShowerC2")
        XDI3 D -1
        Stop

    death.Bloody:
        XDI3 P  8
        XDI3 QQQQQ 0 A_CustomMissile("EnemySplashGus3",random(4,32),0,random(0,360),2,random(0,360))
        XDI3 Q  0 A_Scream
        XDI3 QQQQ 1 A_SpawnDebris("BloodShowerC2")
        XDI3 Q 0 A_CustomMissile("EnemySplashGus3",random(40,48),0,random(0,360),2,random(60,90))
        XDI3 QQQQ 1 A_SpawnDebris("BloodShowerC2")
        XDI3 Q 0 A_CustomMissile("EnemySplashGus3",random(40,48),0,random(0,360),2,random(60,90))
        XDI3 QQQQ 1 A_SpawnDebris("BloodShowerC2")
        XDI3 R 0 A_CustomMissile("EnemySplashGus3",random(40,48),0,random(0,360),2,random(60,90))
        XDI3 RRRR 1 A_SpawnDebris("BloodShowerC2")
        XDI3 R 0 A_CustomMissile("EnemySplashGus3",random(40,48),0,random(0,360),2,random(60,90))
        XDI3 RRRR 1 A_SpawnDebris("BloodShowerC2")
        XDI3 S 0 A_NoBlocking
        XDI3 SSSS 2 A_SpawnDebris("BloodShowerC2")
        XDI3 TTTT 2 A_SpawnDebris("BloodShowerC2")
        XDI3 UUUU 2 A_SpawnDebris("BloodShowerC2")
        XDI3 VVVV 2 A_SpawnDebris("BloodShowerC2")
        XDI3 V -1 A_BossDeath  
        Stop
    death.hunter:
        BOS2 H 1
        XDI3 E 0 A_PlaySound("enemy/bisected")
        XDI3 E 0 A_Jump(128,1)
        goto death.kubiuti+4
        XDI3 L 0 A_Fall
        XDI3 LLLLL 0 A_CustomMissile("EnemySplashGus3",random(52,60),0,random(0,360),2,random(0,360))
        XDI3 L 1 A_SpawnItem("BOS2Kubiuted2",0,32,0)
        XDI3 LLLLLLLLLL 1 A_ThrowGrenade ("BloodSplashGus1c",-8,5+random(0,7),8)
        XDI3 O 0 A_CustomMissile("EnemySplashGus3",random(40,48),0,random(0,360),2,random(60,90))
        XDI3 MMMMMNNNNN 1 A_ThrowGrenade ("BloodSplashGus1c",-12,5+random(0,7),8)
        XDI3 O 0 A_CustomMissile("EnemySplashGus3",random(24,32),0,random(0,360),2,random(60,90))
        XDI3 OOOOOOOOOO 1 A_ThrowGrenade ("BloodSplashGus1c",-24,5+random(0,7),8)
    Dhunter:
        XDI3 O 0 A_CustomMissile("EnemySplashGus3",random(16,24),0,random(0,360),2,random(60,90))
        XDI3 OO 2 A_ThrowGrenade ("BloodSplashGus1c",-24,5+random(0,7),8)
        XDI3 O 0 A_Jump(16,1)
        Loop
    Dhunter2:
        XDI3 O 4 A_ThrowGrenade ("BloodSplashGus1c",-24,5+random(0,7),8)
        XDI3 O 0 A_Jump(16,1)
        Loop    
        XDI3 O -1
        Stop
    death.kubiuti:
        TNT1 A 0 A_GiveToTarget("SuperMeterCounter",3)
        BOS2 H 1
        TNT1 A 0 A_Jump(128,"Death.Hunter","Death.Bloody","Death.Bloody2","XDeath")    
        XDI3 E 0 A_PlaySound("enemy/bisected")
        XDI3 E 0 A_Fall
        XDI3 EEEEEEEEEE 0 A_CustomMissile("EnemySplashGus3",random(52,60),0,random(0,360),2,random(0,360))
        XDI3 E 1 A_SpawnItem("BOS2Kubiuted",0,56,0)
        XDI3 EEEEEEEEEE 1 A_ThrowGrenade ("BloodSplashGus1c",56,5+random(0,7),8)
        XDI3 E 0 A_CustomMissile("EnemySplashGus3",random(24,32),0,random(0,360),2,random(60,90))
        XDI3 EEEEEEEEEE 1 A_ThrowGrenade ("BloodSplashGus1c",-8,5+random(0,7),8)
        XDI3 F 0 A_CustomMissile("EnemySplashGus3",random(24,32),0,random(0,360),2,random(60,90))
        XDI3 FFFFFFFFFF 1 A_ThrowGrenade ("BloodSplashGus1c",-8,5+random(0,7),8)
        XDI3 G 0 A_CustomMissile("EnemySplashGus3",random(24,32),0,random(0,360),2,random(60,90))
        XDI3 GGGG 2 A_ThrowGrenade ("BloodSplashGus1c",-8,5+random(0,7),8)
        goto death+3
    }
}

